@model IEnumerable<MosadMVCServer.Models.Agent>

@{
	ViewData["Title"] = "Agents";
}

<h1>Agents</h1>

<p>
	<a asp-action="Create">Create New</a>
</p>
<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Id)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Nickname)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.PhotoUrl)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Location.X)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Location.Y)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Status)
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Id)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Nickname)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.PhotoUrl)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Location.X)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Location.Y)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Status)
				</td>
				<td>
					
					@Html.ActionLink("Details", "Details", new { id=item.Id }) 

				</td>
			</tr>
		}
	</tbody>
</table>
